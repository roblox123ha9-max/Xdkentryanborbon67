Tab7Section:NewButton("Buy Coloured Triven(300million!)", "Buys Coloured Trivuen", function()
local args = {
[1] = "purchase",
[2] = 40096, -- item ID
[3] = 1 -- amount
}
game:GetService("ReplicatedStorage").Events.Coin.CoinMarketRE:FireServer(unpack(args))
end)

Tab7Section:NewButton("Dupe Colour Triven(free)", "Dupes", function()
local args = {
[1] = "40096",
[2] = 6,
[3] = "Colored"
}
game:GetService("ReplicatedStorage").Functions.Upgrade.UpgradeRF:InvokeServer(unpack(args))
end)

Tab7Section:NewButton("Buy colored Dominus", "Requires idk", function()
local args = {
[1] = "purchase",
[2] = 40040,
[3] = 1
}

game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("Coin"):WaitForChild("CoinMarketRE"):FireServer(unpack(args)) 

end)


